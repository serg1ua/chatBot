    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "co-mocha": "^1.2.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.10",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
  "engines": {
    "node": "10.14.1",
    "npm": "6.6.0"
  }
